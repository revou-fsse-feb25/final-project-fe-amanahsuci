    <script type="module">
        import PhotoSwipeLightbox from '/assets/js/photoswipe/photoswipe-lightbox.esm.js?v=1';
        const leftArrowSVGString = '';
        const options = {
            arrowPrevSVG: leftArrowSVGString,
            arrowNextSVG: leftArrowSVGString,
            gallery: '#pswp-gallery',
            children: 'figure a',
            showHideAnimationType: 'zoom',
            initialZoomLevel: .7,
            pswpModule: () => import('/assets/js/photoswipe/photoswipe.esm.js?v=1')
        };
        const lightbox = new PhotoSwipeLightbox(options);
        lightbox.on('uiRegister', function() {
            lightbox.pswp.ui.registerElement({
                name: 'custom-caption',
                order: 9,
                isButton: false,
                appendTo: 'root',
                html: 'Caption text',
                showHideOpacity: true,
                showHideAnimationType: 'fade',
                getThumbBoundsFn: false,
                onInit: (el, pswp) => {
                    lightbox.pswp.on('change', () => {
                        const currSlideElement = lightbox.pswp.currSlide.data.element;
                        let captionHTML = '';
                        if (currSlideElement) {
                            const hiddenCaption = currSlideElement.querySelector(
                                '.detailsImgCaption');
                            if (hiddenCaption) {
                                // get caption from element with class hidden-caption-content
                                captionHTML = hiddenCaption.innerHTML;
                            } else {
                                // get caption from alt attribute
                                captionHTML = currSlideElement.querySelector('img')
                                    .getAttribute('alt');
                            }
                        }
                        el.innerHTML = captionHTML || '';
                    });
                }
            });
        });
        lightbox.init();
    </script>
        <script>
            $(document).ready(function() {
                // function to show package if not premium yet
                let body = $("body")
                prelock = $('#prelock');
                premiumContent = document.getElementById("premiumContent");
                homeLink = document.querySelector("#firstNav");
                homeLinkData = homeLink.getAttribute('data-url');
                // console.log(homeLinkData);
                homeNav = document.querySelector("#firstNav span");
                lastScrollTop = 0;

                function scrollLocked() {
                    var a = $(window).scrollTop();
                    var b = $(".-parallax").offset().top;

                    if (a > b) {
                        body.addClass("locked");
                        prelock.addClass("active");
                        homeNav.style.display = "none";
                        premiumContent.style.display = "block";
                        homeLink.setAttribute("href", "https://premium.bisnis.com");
                        document.querySelectorAll(".navLink").forEach((el, index) => {
                            let sub = el.innerHTML.trim();
                            // let linkLabel = el.dataset.label;
                            if (index !== 0 && document.querySelectorAll(".navLink").length > 5) {
                                // console.log(linkLabel);
                                el.innerHTML = sub.substring(0, 5) + '..';
                            }
                        });
                    } else {
                        body.removeClass("locked")
                        prelock.removeClass("active");
                    }
                }

                function scrollUnlock() {
                    c = prelock.scrollTop();
                    if (c === 0) {
                        body.removeClass("locked")
                        prelock.removeClass("active");
                        homeNav.style.display = "block";
                        premiumContent.style.display = "none";
                        homeLink.setAttribute("href", homeLinkData);
                        document.querySelectorAll(".navLink").forEach((el, index) => {
                            let linkLabel = el.dataset.label;
                            if (index !== 0 && document.querySelectorAll(".navLink").length > 5) {
                                el.innerHTML = linkLabel;
                            }
                        });
                    }
                }
                prelock.scroll(function() {
                    scrollUnlock()
                })
                $(window).scroll(function() {
                    let st = $(this).scrollTop();
                    if (st > lastScrollTop) {
                        scrollLocked()
                    }
                    lastScrollTop = st;
                })
            });

            $('.package-btn').on('click', function() {
                console.log($('#isLogin').val());
                if ($('#isLogin').val() == 0) {
                    location.href =
                        "https://id.bisnis.com/?referer=ZmluYW5zaWFsLmJpc25pcy5jb20vcmVhZC8yMDI0MDEwMy85MC8xNzI5MjUzL2xhYmEtYmFuay1qdW1iby1tYXNpaC1sZWJpaC1iZXNhci1kYXJpLWdhYnVuZ2FuLTEzLWJhbmsta2JtaS1paWk=";
                } else {
                    $.ajax({
                        type: "POST",
                        url: "https://finansial.bisnis.com/api/v1/xendit-payment",
                        data: {
                            product_id: $(this).data('productid'),
                            product_price_id: $(this).data('priceid'),
                            success_redirect_url: "https://finansial.bisnis.com/read/20240103/90/1729253/laba-bank-jumbo-masih-lebih-besar-dari-gabungan-13-bank-kbmi-iii" + '?payment=success',
                            failure_redirect_url: "https://finansial.bisnis.com/read/20240103/90/1729253/laba-bank-jumbo-masih-lebih-besar-dari-gabungan-13-bank-kbmi-iii" + '?payment=failed',
                        },
                        success: function(data) {
                            if (data === 'false') {
                                alert('Terjadi gangguan, silahkan coba kembali!');
                            } else {
                                location.href = data;
                            }
                        },
                        error: function(e) {
                            alert('Terjadi kesalahan, silahkan coba kembali!');
                        }
                    });
                }
            });

            $('#refresh').on('click', function() {
                location.reload();
            });
        </script>
